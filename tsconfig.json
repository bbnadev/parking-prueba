{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@icons/*": [
        "src/components/icons/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}